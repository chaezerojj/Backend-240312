package chapter11;

// 멤버변수를 갖는 Student 클래스 정의
public class Exercise6_2 {

	class Student {
		String name;
		int ban;
		int no;
		int kor;
		int eng;
		int math;

		public void info(String name, int ban, int no, int kor, int eng, int math) {
			this.name = name;
			this.ban = ban;
			this.no = no;
			this.kor = kor;
			this.eng = eng;
			this.math = math;
		}
		
		
	}

	public static void main(String[] args) {
		Student s = new Student();
		
		String str = s.info();
		System.out.println(str);
		
		
	}

}
