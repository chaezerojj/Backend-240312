package chapter11;

// 멤버변수를 갖는 Student 클래스 정의
public class Quiz1 {
	
	class Student {
		String name;
		int ban;
		int no;
		int kor;
		int eng;
		int math;
	}
	
	private void info() {
		// TODO Auto-generated method stub

	}
	
	public static void main(String[] args) {
		Student s = new Student("홍길동", 1, 1, 100, 68, 76);
		
		String str = s.info();
		System.out.println(str);
		
		
	}

}
