package chapter19;

public class Exercise9_4 {
	public boolean contains(String src, String target) {
		int str = 0;
		str = src.indexOf(target, str);
		
		if (src.contains(target)) {
			return true;
		}
		return false;
	}
	
	
	public static void main(String[] args) {
		
		System.out.println();
		System.out.println(contains("12345", "67"));
	}

}
