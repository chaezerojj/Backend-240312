package chapter04;

public class SwitchExample {

	public static void main(String[] args) {
		//? switch문
		/*
		 * switch(변수) {
		 * 		case 값1: 
		 * 				실행문A
		 * 				break;
		 * 		case 값2: 
		 * 				실행문B
		 * 				break;
		 * 		default:
		 * 				실행문C
		 * }
		 * 
		 * */
		// 변수 == 값2
		int num1 = 1;
		switch (num1) {
		case 1:
			System.out.println("num1: " + num1);
			break;
		case 2:
			System.out.println("num1: " + num1);
			break;
		default:
			System.out.println("error");
		}
		
		System.out.println();
		
		int time = 11;
		switch(time) {
		case 8:
			System.out.println("출근");
			break;
		case 9:
			System.out.println("회의 시작");
			break;
		case 10:
			System.out.println("업무");
			break;
		default:
			System.out.println("외근");
			break;
		}
		
		
		

	}

}
