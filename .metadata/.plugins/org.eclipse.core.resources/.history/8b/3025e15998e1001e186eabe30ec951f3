package chapter04;

public class ArithOp {

	public static void main(String[] args) {
		//? 연산자
		// 대입 연산자와 산술 연산자
		// = >> 연산자 오른쪽에 있는 값을 연산자 왼쪽에 대입
		// + >> 두개의 피연산자의 값을 더함
		// - >> 빼기
		// * >> 곱하기
		// / >> 나누기
		// % >> 나머지를 반환하는 연산자
		
		int n1 = 10;
		int n2 = 3;
		int result = n1 / n2;
		System.out.println(result);
		int r1 = n1 % n2;
		System.out.println(r1);
		
		//? 연산자의 우선순위
		int r2 = 2 - 1 - 3 * 2;
		System.out.println(r2);
	}

}
